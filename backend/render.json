{
  "name": "student-analyst-backend",
  "type": "web",
  "env": "node",
  "region": "oregon",
  "plan": "free",
  "buildCommand": "npm install",
  "startCommand": "npm start",
  "healthCheckPath": "/health",
  "envVars": [
    {
      "key": "NODE_ENV",
      "value": "production"
    },
    {
      "key": "PORT",
      "value": "10000"
    }
  ],
  "scaling": {
    "minInstances": 0,
    "maxInstances": 1
  }
} 